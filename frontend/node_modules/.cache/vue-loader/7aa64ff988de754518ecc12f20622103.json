           />\n                  {{ words.signin.google }}\n                </v-btn>\n              </v-col>\n            </v-row>\n\n            <v-row>\n              <v-col>\n                <v-btn\n                  class=\"text-capitalize caption\"\n                  height=\"48px\"\n                  width=\"100%\"\n                  outlined\n                  style=\"border-color: #979797\"\n                  tile\n                >\n                  <img\n                    class=\"button-logo-img mr-4\"\n                    :src=\"path_facebook\"\n                    style=\"height: 25px\"\n                  />\n                  {{ words.signin.facebook }}\n                </v-btn>\n              </v-col>\n            </v-row>\n\n            <v-row>\n              <v-col>\n                <v-divider></v-divider>\n              </v-col>\n            </v-row>\n\n            <ValidationObserver ref=\"obs\" v-slot=\"ObserverProps\">\n              <v-row class=\"regist_info pt-6\">\n                <v-col>\n                  <ValidationProvider\n                    :rules=\"MyEnum.validateNames().requiredEmail\"\n                    v-slot=\"{ errors }\"\n                    :name=\"words.signin.mail\"\n                  >\n                    <v-text-field\n                      v-model=\"email\"\n                      dense\n                      height=\"48px\"\n                      outlined\n                      required\n                      :error-messages=\"errors\"\n                      :placeholder=\"words.signin.mail\"\n                    ></v-text-field>\n                  </ValidationProvider>\n\n                  <ValidationProvider\n                    :rules=\"MyEnum.validateNames().required\"\n                    v-slot=\"{ errors }\"\n                    :name=\"words.signin.password\"\n                  >\n                    <v-text-field\n                      v-model=\"password\"\n                      dense\n                      height=\"48px\"\n                      :append-icon=\"show1 ? 'mdi-eye' : 'mdi-eye-off'\"\n                      :type=\"show1 ? 'text' : 'password'\"\n                      outlined\n                      required\n                      :placeholder=\"words.signin.password\"\n                      :error-messages=\"errors\"\n                      @click:append=\"show1 = !show1\"\n                    ></v-text-field>\n                  </ValidationProvider>\n\n                  <v-btn\n                    class=\"caption\"\n                    color=\"#049cfc\"\n                    depressed\n                    height=\"48px\"\n                    width=\"100%\"\n                    tile\n                    :disabled=\"!ObserverProps.valid\"\n                    @click=\"login\"\n                  >\n                    {{ words.signin.login }}\n                  </v-btn>\n                </v-col>\n              </v-row>\n            </ValidationObserver>\n\n            <v-row>\n              <v-col>\n                <v-divider></v-divider>\n                <p @click=\"signUpSwitch\">\n                  <span class=\"blue--text\">{{ words.signin.signup }}</span>\n                  {{ words.signin.message }}\n                </p>\n              </v-col>\n            </v-row>\n            <v-row>\n              <v-col>\n                <p>\n                  <span class=\"blue--text\">{{ words.signin.guest }}</span>\n                  {{ words.signin.message }}\n                </p>\n              </v-col>\n            </v-row>\n          </div>\n        </div>\n      </div>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport MyEnum from \"@/common/myEnum.js\";\nimport totalApis from \"@/common/totalApis.js\";\nimport words from \"@/words/words.json\";\n\nexport default {\n  data: () => ({\n    words: words,\n    MyEnum: MyEnum,\n    email: null,\n    password: null,\n    path_google: require(\"@/assets/google_logo-48.png\"),\n    path_facebook: require(\"@/assets/facebook_logo_58.png\"),\n    show1: false,\n  }),\n  methods: {\n    signUpSwitch: function () {\n      this.$router.push(MyEnum.menuPages().SignUp);\n    },\n    login() {\n      axios({\n        method: \"post\",\n        url: totalApis.apis().token,\n        data: {\n          email: this.email,\n          password: this.password,\n        },\n      })\n        .then((response) => {\n          const status = response?.status;\n          const result = response?.data;\n          console.log(response);\n          if (status == 200) {\n            localStorage.setItem(\"JWT\", result.token);\n            console.log(localStorage.getItem(\"JWT\"));\n            this.getMyself();\n          } else {\n            console.log(response);\n          }\n        })\n        .catch(function (error) {\n          console.log(error);\n        });\n      this.email = \"\";\n      this.password = \"\";\n    },\n    getMyself: function () {\n      axios({\n        method: \"get\",\n        url: totalApis.apis().userMyself,\n        headers: {\n          Authorization: `JWT ${localStorage.getItem(\"JWT\")}`,\n        },\n      })\n        .then((response) => {\n          const userData = response?.data;\n          localStorage.setItem(\"userName\", userData.name);\n          console.log(localStorage.getItem(\"userName\"));\n          localStorage.setItem(\"userId\", userData.id);\n          console.log(localStorage.getItem(\"userId\"));\n          this.$router.push(\"/\");\n        })\n        .catch(function (error) {\n          this.tokenRefresh();\n          console.log(error);\n        });\n    },\n  },\n};\n</script>\n\n<style>\n.regist_info {\n  margin-top: 40px;\n}\n</style>\n"]}]}