at-left mt-5\">\n          <v-btn class=\"primary lighten-1 font-weight-bold\" @click=\"back\">\n            {{ words.confirm.back }}\n          </v-btn>\n        </div>\n\n        <div class=\"float-right mt-5\">\n          <v-btn class=\"primary lighten-1 font-weight-bold\" @click=\"updateName\">\n            {{ words.confirm.update }}\n          </v-btn>\n        </div>\n      </div>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport totalPages from \"@/common/totalPages.js\";\nimport words from \"@/words/words.json\";\n\nexport default {\n  name: \"Confirm\",\n  data: () => ({\n    totalPages: totalPages,\n    words: words,\n    userName: \"\",\n    mail: \"\",\n    password: \"\",\n  }),\n  props: {\n    userName: {\n      type: String,\n      default: \"\",\n    },\n    mail: {\n      type: String,\n      default: \"\",\n    },\n    password: {\n      type: String,\n      default: \"\",\n    },\n  },\n  methods: {\n    back: function () {\n      this.$emit(\"back\", 1);\n    },\n    updateName: function () {\n      this.$emit(\"update-name\", this.userName, this.mail, this.password);\n    },\n  },\n};\n</script>\n\n<style>\n#Confirm {\n  width: 100%;\n  height: auto;\n}\n</style>\n"]}]}