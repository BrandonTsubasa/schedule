\n                </v-btn>\n              </v-col>\n            </v-row>\n\n            <v-row>\n              <v-col>\n                <v-divider></v-divider>\n              </v-col>\n            </v-row>\n\n            <ValidationObserver ref=\"obs\" v-slot=\"ObserverProps\">\n              <v-row class=\"regist_info pt-6\">\n                <v-col>\n                  <ValidationProvider\n                    :rules=\"MyEnum.validateNames().required\"\n                    v-slot=\"{ errors }\"\n                    :name=\"words.signup.username\"\n                  >\n                    <v-text-field\n                      v-model=\"username\"\n                      autofocus\n                      dense\n                      height=\"48px\"\n                      outlined\n                      required\n                      :error-messages=\"errors\"\n                      :label=\"words.signup.username\"\n                    ></v-text-field>\n                  </ValidationProvider>\n\n                  <ValidationProvider\n                    :rules=\"MyEnum.validateNames().requiredEmail\"\n                    v-slot=\"{ errors }\"\n                    :name=\"words.signup.mail\"\n                  >\n                    <v-text-field\n                      v-model=\"email\"\n                      dense\n                      height=\"48px\"\n                      outlined\n                      required\n                      :error-messages=\"errors\"\n                      :label=\"words.signup.mail\"\n                    ></v-text-field>\n                  </ValidationProvider>\n\n                  <!-- <ValidationObserver>\n                    <ValidationProvider\n                      :rules=\"`${\n                        MyEnum.validateNames().requiredPassword\n                      }|confirmed:password2`\"\n                      v-slot=\"{ errors }\"\n                      :name=\"words.signup.password\"\n                    > -->\n                  <v-text-field\n                    v-model=\"password\"\n                    dense\n                    height=\"48px\"\n                    :append-icon=\"show1 ? 'mdi-eye' : 'mdi-eye-off'\"\n                    :type=\"show1 ? 'text' : 'password'\"\n                    outlined\n                    required\n                    :error-messages=\"errors\"\n                    :label=\"words.signup.password\"\n                    @click:append=\"show1 = !show1\"\n                    counter=\"20\"\n                  ></v-text-field>\n                  <!-- </ValidationProvider> -->\n\n                  <!-- <ValidationProvider v-slot=\"{ errors }\" vid=\"password2\"> -->\n                  <v-text-field\n                    v-model=\"password2\"\n                    dense\n                    height=\"48px\"\n                    :append-icon=\"show1 ? 'mdi-eye' : 'mdi-eye-off'\"\n                    :type=\"show2 ? 'text' : 'password'\"\n                    outlined\n                    required\n                    :error-messages=\"errors\"\n                    :label=\"words.signup.password2\"\n                    @click:append=\"show2 = !show2\"\n                    counter=\"20\"\n                  ></v-text-field>\n                  <!-- </ValidationProvider>\n                  </ValidationObserver> -->\n\n                  <v-btn\n                    class=\"caption\"\n                    color=\"#049cfc\"\n                    depressed\n                    height=\"48px\"\n                    width=\"100%\"\n                    tile\n                    :disabled=\"!ObserverProps.valid\"\n                    @click=\"regist\"\n                  >\n                    {{ words.signup.regist }}\n                  </v-btn>\n                </v-col>\n              </v-row>\n            </ValidationObserver>\n\n            <v-row>\n              <v-col>\n                <v-divider></v-divider>\n                <p @click=\"signInSwitch\">\n                  <span class=\"blue--text\">{{ words.signup.login }}</span>\n                  {{ words.signup.message }}\n                </p>\n              </v-col>\n            </v-row>\n            <v-row>\n              <v-col>\n                <p>\n                  <span class=\"blue--text\">{{ words.signup.guest }}</span>\n                  {{ words.signup.message }}\n                </p>\n              </v-col>\n            </v-row>\n          </div>\n        </div>\n      </div>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport MyEnum from \"@/common/myEnum.js\";\nimport totalApis from \"@/common/totalApis.js\";\nimport words from \"@/words/words.json\";\n\nexport default {\n  data: () => ({\n    words: words,\n    MyEnum: MyEnum,\n    username: null,\n    email: null,\n    password: null,\n    password2: null,\n    path_google: require(\"@/assets/google_logo-48.png\"),\n    path_facebook: require(\"@/assets/facebook_logo_58.png\"),\n    show1: false,\n    show2: false,\n  }),\n  methods: {\n    signInSwitch: function () {\n      this.$router.push(MyEnum.menuPages().SignIn);\n    },\n    regist() {\n      axios({\n        method: \"post\",\n        url: totalApis.apis().userCreate,\n        data: {\n          name: this.username,\n          email: this.email,\n          password: this.password,\n        },\n      })\n        .then((response) => {\n          console.log(response);\n        })\n        .catch(function (error) {\n          console.log(error);\n        });\n      this.username = \"\";\n      this.email = \"\";\n      this.password = \"\";\n      this.password2 = \"\";\n    },\n  },\n};\n</script>\n\n<style>\n.regist_info {\n  margin-top: 40px;\n}\n</style>\n"]}]}