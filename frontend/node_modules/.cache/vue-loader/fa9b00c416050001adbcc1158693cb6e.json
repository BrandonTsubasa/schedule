et-y\n          min-width=\"auto\"\n          >\n          <template v-slot:activator=\"{ on, attrs }\">\n            <v-text-field\n              v-model=\"date\"\n              label=\"期日\"\n              prepend-icon=\"mdi-calendar\"\n              readonly\n              v-bind=\"attrs\"\n              v-on=\"on\"\n            ></v-text-field>\n          </template>\n          <v-date-picker\n            v-model=\"date\"\n            :active-picker.sync=\"activePicker\"\n            @change=\"save\"\n          ></v-date-picker>\n        </v-menu>\n        </v-col>\n\n\n        <v-col cols=\"2\">\n          <v-select\n          :items=\"name\"\n          v-model=\"name\"\n          label=\"名前\">\n          </v-select>\n        </v-col>\n      </v-row>\n      <v-row>\n        <v-col>\n          <v-btn medium id=\"btnColor\" @click=\"addTodo\">\n            追加\n          </v-btn>\n        </v-col>\n      </v-row>\n    </v-container>\n\n    <v-container>\n      <v-card class=\"py-5\">\n        <v-row>\n          <v-col>\n            <v-data-table\n            :headers=\"headers\"\n            :items=\"tableDatas\"\n            :loading=\"loading\"\n            locale=\"ja\"\n            >\n            <template v-slot:[`item.endDate`]=\"{ item }\">\n              <span>{{ new Date(item.endDate).toLocaleString() }}</span>\n            </template>\n            </v-data-table>\n          </v-col>\n        </v-row>\n      </v-card>\n    </v-container>\n  </div>\n</template>\n\n<script>\n// import moment from \"moment\";\nimport axios from 'axios'\n\nexport default {\n  name: \"task\",\n  data: () => ({\n    loading: false,\n    title: \"\",\n    priority: [\"高\", \"中\", \"低\"],\n    status: [\"未着手\", \"完了\"],\n    name: [\"テスト１\", \"テスト2\", \"テスト3\"],\n    today: new Date(),\n    activePicker: null,\n    date: null,\n    menu: false,\n    headers : [\n      {\n        text: \"ID\",\n        value: \"id\",\n      },\n      {\n        text: \"タスク名\",\n        value: \"title\",\n      },\n      {\n        text: \"優先度\",\n        value: \"priority\",\n      },\n      {\n        text: \"状態\",\n        value: \"status\",\n      },\n      {\n        text: \"期日\",\n        value: \"endDate\",\n      },\n      {\n        text: \"名前\",\n        value: \"name\",\n      }\n    ],\n    tableDatas: [],\n  }),\n  watch: {\n    menu (val) {\n      val && setTimeout(() => (this.activePicker = 'YEAR'))\n    },\n  },\n  methods: {\n    addTodo: function() {\n      if (this.title === \"\") {\n        alert(\"作業名を入力してください\");\n        return;\n      }\n      axios.post(\"/api/task/create/\", {\n        // task_name: this.task_name,\n        // name: this.username,\n        // status: this.status,\n        // priority: this.priority,\n        // end_date: this.end_date,\n        headers: { Authorization: `JWT ${localStorage.getItem(\"JWT\")}` }\n      })\n      .then(response => {\n        // console.log(\"response_data\")\n        console.log(response.data);\n        console.log(localStorage.getItem(\"JWT\"));\n        console.log(\"Authenticated\");\n      })\n      .catch(function (error){\n        console.log(error);\n      })\n    },\n    save(date) {\n      this.$ref.menu.save(date)\n    },\n    \n    // allowedDate: function(today) {\n    //   let today = new Date() \n    //   today = new Date(\n    //     today.getFullYear(),\n    //     today.getMonth(),\n    //     today.getDate()\n    //   )\n    // },\n  },\n};\n</script>\n<style scoped>\n#task {\n  width: 100%;\n  height: 100%;\n}\nh1 {\n  height: 50px;\n}\n#btnColor {\n  border-color: #0474bd;\n  background-color: #049cfc;\n  color: white;\n}\n.txt-color{\n  color: #808080;\n}\n</style>"]}]}