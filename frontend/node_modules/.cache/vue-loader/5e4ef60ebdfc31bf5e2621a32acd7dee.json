 </v-list-item>\n          <v-list-item  @click=\"Logout\">\n            <v-list-title>{{ words[5] }}</v-list-title>\n          </v-list-item>\n        </v-list-item-group>\n      </v-navigation-drawer>\n      \n    </header>\n    <v-main class=\"mt-5\">\n      <div class=\"page-switch\">\n        <v-btn\n        class=\"text-capitalize caption font-weight-bold\"\n        height=\"48px\"\n        color=\"grey darken-1\"\n        outlined\n        style=\"border-color:#979797;\"\n        tile\n        x-large\n        @click=\"taskSwitch\"\n        >\n        {{ words[6] }}\n        </v-btn>\n\n        <v-btn\n        class=\"text-capitalize caption font-weight-bold\"\n        height=\"48px\"\n        color=\"grey darken-1\"\n        outlined\n        style=\"border-color:#979797;\"\n        tile\n        x-large\n        @click=\"calSwitch\"\n        >\n        {{ words[7] }}\n        </v-btn>\n\n      </div>\n    </v-main>\n\n    <router-view></router-view>\n\n  </v-app>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport MyApi from \"@/common/myApi.js\";\nimport MyEnum from \"@/common/myEnum.js\";\n\n\nexport default {\n  name: \"app\",\n  data: () => ({\n    drawer: false,\n    loginUser: null,\n    result: null,\n    words: [\n      \"Schedule Management\",\n      \"ログイン: \",\n      \"新規登録\",\n      \"ログイン\",\n      \"グループ\",\n      \"ログアウト\",\n      \"Task\",\n      \"Calendar\",\n    ]\n  }),\n\n  methods: {\n    taskSwitch: function() {\n      this.$router.push(MyEnum.pages().Task);\n    },\n    calSwitch: function() {\n      this.$router.push(MyEnum.pages().TaskCalendar);\n    },\n    signUpSwitch: function() {\n      this.$router.push(MyEnum.menuPages().SignUp);\n    },\n    signInSwitch: function() {\n      this.$router.push(MyEnum.menuPages().SignIn);\n    },\n    GroupSwitch: function() {\n      this.$router.push(MyEnum.menuPages().Group);\n    },\n    Logout: function() {\n      localStorage.clear();\n      this.loginUser = \"\";\n      this.$router.push(MyEnum.menuPages().SignIn);\n    },\n    getMyself: function() {\n\n      axios({\n        method: \"get\",\n        url: MyApi.apis().userMyself,\n        headers: {\n          Authorization: `JWT ${localStorage.getItem(\"JWT\")}`\n        },\n      })\n      .then(response =>{\n        const userData = response?.data;\n        this.loginUser = userData.name;\n        localStorage.setItem(\"userId\", userData.id);\n      })\n      .catch(function (error){\n        console.log(error);\n      })\n    },\n    tokenCheck: function(){\n      const token = localStorage.getItem(\"JWT\");\n      if (!token){\n        // トークンがない場合はログイン画面へ\n        this.$router.push(MyEnum.menuPages().SignIn);\n      }\n      else{\n        // トークンがある場合は検証\n        axios({\n          method: \"post\",\n          url: MyApi.apis().tokenVerify,\n          data: {\n            token\n          }\n        })\n        .then(response =>{\n          // レスポンスが200ならTask画面へ遷移\n          const status = response?.status;\n          if (status === 200){\n            this.$router.push(MyEnum.pages().Task);\n          }\n        })\n        .catch(function(error){\n          this.result = error.status;\n        })\n\n        // レスポンスが401ならトークンをリフレッシュ\n        if(this.result === 401){\n          axios({\n            method: \"post\",\n            url: MyApi.apis().tokenRefresh,\n            data: {\n              token\n            },\n          })\n          .then(response =>{\n            console.log(response);\n          })\n          .catch(function(error){\n            console.log(error);\n          })\n        }\n        else{\n          this.$router.push(MyEnum.menuPages().SignIn);\n        }\n      }\n    },\n  },\n  mounted: function() {\n    this.getMyself();\n  },\n  beforeCreate: function() {\n    this.tokenCheck();\n  },\n\n};\n</script>\n\n<style scoped>\n#app {\n  width: 100%;\n  height: 100%;\n  background-color: #cccccc;\n}\n.page-switch{\n  text-align: center;\n}\n</style>"]}]