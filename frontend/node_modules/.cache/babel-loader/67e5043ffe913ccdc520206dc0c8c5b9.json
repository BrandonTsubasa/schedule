\n\n    <v-container class=\"mt-10\" v-if=\"pages === 2\">\n      <v-card>\n        <v-row justify=\"center\" class=\"pt-5\">\n          <v-col cols=\"8\">\n            <InputPassword\n              :text1=\"words.userPasswordEdit.password\"\n              :message1=\"words.userPasswordEdit.back\"\n              :password=\"password\"\n              @back-pages=\"back\"\n              :message2=\"words.userPasswordEdit.next\"\n              @input-text=\"addInfo\"\n            />\n          </v-col>\n        </v-row>\n      </v-card>\n    </v-container>\n\n    <v-container class=\"mt-10\" v-if=\"pages === 3\">\n      <v-card>\n        <v-row justify=\"center\" class=\"pt-5\">\n          <v-col cols=\"8\">\n            <Confirm\n              :userName=\"user\"\n              :mail=\"email\"\n              :password=\"password\"\n              @back=\"returnPages\"\n              @update-name=\"update\"\n            />\n          </v-col>\n        </v-row>\n      </v-card>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport InputPasswordInfo from \"@/components/input/InputPasswordInfo.vue\";\nimport InputPassword from \"@/components/input/InputPassword.vue\";\nimport Confirm from \"@/components/confirm/Confirm.vue\";\nimport totalApis from \"@/common/totalApis.js\";\nimport totalPages from \"@/common/totalPages.js\";\nimport words from \"@/words/words.json\";\n\nexport default {\n  name: \"UserPasswordEdit\",\n  data: () => ({\n    words: words,\n    totalPages: totalPages,\n    pages: 1,\n    user: \"\",\n    email: \"\",\n    password: \"\",\n    getMail: \"\",\n  }),\n  components: {\n    InputPasswordInfo,\n    InputPassword,\n    Confirm,\n  },\n  methods: {\n    addTwoInfos: function (value1, value2) {\n      this.user = value1;\n      this.email = value2;\n      console.log(this.user);\n      console.log(this.email);\n      this.pages++;\n    },\n    addInfo: function (value) {\n      this.password = value;\n      console.log(this.password);\n      this.pages++;\n    },\n    back: function (value) {\n      this.password = \"\";\n      const i = value;\n      this.pages = this.pages - i;\n    },\n    returnPages: function (value) {\n      const i = value;\n      this.pages = this.pages - i;\n    },\n    update: function (value1, value2, value3) {\n      const newName = value1;\n      console.log(newName);\n      const newEmail = value2;\n      console.log(newEmail);\n      const newPassword = value3;\n      console.log(newPassword);\n\n      axios({\n        method: \"put\",\n        url: totalApis.apis().userMyself,\n        headers: {\n          Authorization: `JWT ${localStorage.getItem(\"JWT\")}`,\n        },\n        data: {\n          email: newEmail,\n          name: newName,\n          password: newPassword,\n        },\n      })\n        .then((response) => {\n          console.log(response);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n      this.user = \"\";\n      this.mail = \"\";\n      this.password = \"\";\n\n      this.$router.push(totalPages.pages().Top);\n    },\n  },\n  beforeMount: function () {\n    axios({\n      method: \"get\",\n      url: totalApis.apis().userMyself,\n      headers: {\n        Authorization: `JWT ${localStorage.getItem(\"JWT\")}`,\n      },\n    })\n      .then((response) => {\n        this.getMail = response?.data?.email;\n      })\n      .catch((error) => {\n        console.log(error);\n      });\n  },\n};\n</script>\n\n<style scoped>\n#UserName {\n  width: 100%;\n  height: 100%;\n}\n</style>\n"],"sourceRoot":"src/views/user"}]}